Config
  { font = "xft:Hasklig:size=11:antialias=true"
  , bgColor =      "#2E3440"
  , fgColor =      "#D8DEE9"
  , border =       BottomB
  , borderColor =  "#646464"
  , position = Top
  , hideOnStart = False
  , allDesktops =      True    -- show on all desktops
  , overrideRedirect = True    -- set the Override Redirect flag (Xlib)
  , pickBroadest =     True   -- choose widest display (multi-monitor)
  , persistent =       True    -- enable/disable hiding (True = disabled)
  , sepChar      = "%"
  , alignSep     = "}{"
  -- %multicpu%
  , template = " %StdinReader% }{  <fc=#020A19></fc> %memory% <fc=#020A19></fc> %enp6s0% <fc=#020A19></fc> %wlp5s0% <fc=#020A19></fc> %alsa:default:Master% <fc=#020A19></fc> %bat0% <fc=#020A19></fc> %date%"
  , commands =
      [
      -- Run MultiCpu [ "--template" , "<fc=#666666><icon=/home/v0d1ch/code/sm4tik-xbm-icons/icons/cpu.xbm/></fc> <total0>% <total1>% <total2>% <total3>% <total4>% <total5>% <total6>% <total7>% <total8>% <total9>% <total10>% <total11>% <total12>%"
      --               , "--Low"      , "50"         -- units: %
      --               , "--High"     , "85"         -- units: %
      --               , "--low"      , "#A3BE8C"
      --               , "--normal"   , "#EBCB8B"
      --               , "--high"     , "#BF616A"
      --               ] 10
       Run Memory [ "--template" ,"<fc=#666666><icon=/home/v0d1ch/code/sm4tik-xbm-icons/icons/mem.xbm/></fc> <usedratio>%"
                   , "--Low"      , "20"
                   , "--High"     , "90"
                   , "--low"      , "#A3BE8C"
                   , "--normal"   , "#EBCB8B"
                   , "--high"     , "#BF616A"
                   ] 10
      , Run Network "enp6s0" ["-t", "<fc=#666666><icon=/home/v0d1ch/code/sm4tik-xbm-icons/icons/net_up_01.xbm/></fc> <rx>KB/<tx>KB", "--Low", "10000", "--low", "#888888", "--High", "800000", "--high", "#f90", "--normal", "#5f4"] 10
      , Run Network "wlp5s0" ["-t", "<fc=#666666><icon=/home/v0d1ch/code/sm4tik-xbm-icons/icons/wifi_02.xbm/></fc> <rx>KB/<tx>KB", "--Low", "10000", "--low", "#888888", "--High", "800000", "--high", "#f90", "--normal", "#5f4"] 10
      , Run StdinReader
      , Run Alsa "default" "Master"
          [ "--template" , "<fc=#666666><icon=/home/v0d1ch/code/sm4tik-xbm-icons/icons/note.xbm/></fc> <status>"
          , "--"
          , "-C" , "#A3BE8C"
          , "-c" , "#BF616A"
          , "-O", "<volume>%"
          , "-o", ""
          , "--highs", ""
          , "--mediums", ""
          , "--lows", ""
          ]
      , Run BatteryN
          ["BAT0"]
          [ "--template" , "<fc=#666666><icon=/home/v0d1ch/code/sm4tik-xbm-icons/icons/bat_full_02.xbm/></fc> <acstatus>"
          , "--Low"      , "10"        -- units: %
          , "--High"     , "80"        -- units: %
          , "--low"      , "#BF616A"
          , "--normal"   , "#EBCB8B"
          , "--high"     , "#A3BE8C"
          ] 50 "bat0"
      , Run Date "<fc=#EBCB8B>%a, %d %b %I:%M %p</fc>" "date" 10
      ]
  }
